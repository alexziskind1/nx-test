{
  "version": 1,
  "projects": {
    "libs": {
      "root": "libs",
      "sourceRoot": "libs",
      "projectType": "library",
      "prefix": "myworkspace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "testing/jest.libs.config.js",
            "tsConfig": "testing/tsconfig.libs.spec.json",
            "passWithNoTests": true,
            "setupFile": "testing/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.libs.json",
              "testing/tsconfig.libs.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "xplat": {
      "root": "xplat",
      "sourceRoot": "xplat",
      "projectType": "library",
      "prefix": "myworkspace",
      "architect": {
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "testing/jest.xplat.config.js",
            "tsConfig": "testing/tsconfig.xplat.spec.json",
            "passWithNoTests": true,
            "setupFile": "testing/test-setup.ts"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "testing/tsconfig.xplat.json",
              "testing/tsconfig.xplat.spec.json"
            ],
            "exclude": [
              "**/node_modules/**"
            ]
          }
        }
      }
    },
    "web-myweb": {
      "projectType": "application",
      "schematics": {
        "@nrwl/angular:component": {
          "style": "scss"
        }
      },
      "root": "apps/web-myweb",
      "sourceRoot": "apps/web-myweb/src",
      "prefix": "myworkspace",
      "architect": {
        "build": {
          "builder": "@angular-devkit/build-angular:browser",
          "options": {
            "outputPath": "dist/apps/web-myweb",
            "index": "apps/web-myweb/src/index.html",
            "main": "apps/web-myweb/src/main.ts",
            "polyfills": "apps/web-myweb/src/polyfills.ts",
            "tsConfig": "apps/web-myweb/tsconfig.app.json",
            "aot": true,
            "assets": [
              "apps/web-myweb/src/favicon.ico",
              "apps/web-myweb/src/assets"
            ],
            "styles": [
              "xplat/web/scss/_index.scss",
              "apps/web-myweb/src/styles.scss"
            ],
            "scripts": []
          },
          "configurations": {
            "production": {
              "fileReplacements": [
                {
                  "replace": "apps/web-myweb/src/environments/environment.ts",
                  "with": "apps/web-myweb/src/environments/environment.prod.ts"
                }
              ],
              "optimization": true,
              "outputHashing": "all",
              "sourceMap": false,
              "extractCss": true,
              "namedChunks": false,
              "extractLicenses": true,
              "vendorChunk": false,
              "buildOptimizer": true,
              "budgets": [
                {
                  "type": "initial",
                  "maximumWarning": "2mb",
                  "maximumError": "5mb"
                },
                {
                  "type": "anyComponentStyle",
                  "maximumWarning": "6kb",
                  "maximumError": "10kb"
                }
              ]
            }
          }
        },
        "serve": {
          "builder": "@angular-devkit/build-angular:dev-server",
          "options": {
            "browserTarget": "web-myweb:build"
          },
          "configurations": {
            "production": {
              "browserTarget": "web-myweb:build:production"
            }
          }
        },
        "extract-i18n": {
          "builder": "@angular-devkit/build-angular:extract-i18n",
          "options": {
            "browserTarget": "web-myweb:build"
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web-myweb/tsconfig.app.json",
              "apps/web-myweb/tsconfig.spec.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/web-myweb/**/*"
            ]
          }
        },
        "test": {
          "builder": "@nrwl/jest:jest",
          "options": {
            "jestConfig": "apps/web-myweb/jest.config.js",
            "tsConfig": "apps/web-myweb/tsconfig.spec.json",
            "passWithNoTests": true,
            "setupFile": "apps/web-myweb/src/test-setup.ts"
          }
        }
      }
    },
    "web-myweb-e2e": {
      "root": "apps/web-myweb-e2e",
      "sourceRoot": "apps/web-myweb-e2e/src",
      "projectType": "application",
      "architect": {
        "e2e": {
          "builder": "@nrwl/cypress:cypress",
          "options": {
            "cypressConfig": "apps/web-myweb-e2e/cypress.json",
            "tsConfig": "apps/web-myweb-e2e/tsconfig.e2e.json",
            "devServerTarget": "web-myweb:serve"
          },
          "configurations": {
            "production": {
              "devServerTarget": "web-myweb:serve:production"
            }
          }
        },
        "lint": {
          "builder": "@angular-devkit/build-angular:tslint",
          "options": {
            "tsConfig": [
              "apps/web-myweb-e2e/tsconfig.e2e.json"
            ],
            "exclude": [
              "**/node_modules/**",
              "!apps/web-myweb-e2e/**/*"
            ]
          }
        }
      }
    },
    "nativescript-mobile": {
      "root": "apps/nativescript-mobile/",
      "sourceRoot": "apps/nativescript-mobile/src",
      "projectType": "application",
      "prefix": "myworkspace",
      "schematics": {
        "@schematics/angular:component": {
          "styleext": "scss"
        }
      },
      "architect": {
        "serve": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "yarn start.nativescript.mobile.preview"
              }
            ]
          }
        },
        "ios": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ns debug ios --no-hmr"
              }
            ],
            "cwd": "apps/nativescript-mobile",
            "parallel": false
          }
        },
        "android": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "ns debug android --no-hmr"
              }
            ],
            "cwd": "apps/nativescript-mobile",
            "parallel": false
          }
        },
        "clean": {
          "builder": "@nrwl/workspace:run-commands",
          "options": {
            "commands": [
              {
                "command": "npx rimraf -- hooks node_modules platforms package-lock.json"
              },
              {
                "command": "npm i"
              },
              {
                "command": "npx rimraf -- package-lock.json"
              }
            ],
            "cwd": "apps/nativescript-mobile",
            "parallel": false
          }
        }
      }
    }
  },
  "cli": {
    "defaultCollection": "@nstudio/xplat"
  },
  "schematics": {
    "@nrwl/angular:application": {
      "unitTestRunner": "jest",
      "e2eTestRunner": "cypress"
    },
    "@nrwl/angular:library": {
      "unitTestRunner": "jest"
    }
  }
}
