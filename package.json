{
  "name": "myworkspace",
  "version": "0.0.0",
  "license": "MIT",
  "scripts": {
    "nx": "nx",
    "start": "nx serve",
    "build": "nx build",
    "test": "nx test",
    "lint": "nx workspace-lint && nx lint",
    "e2e": "nx e2e",
    "affected:apps": "nx affected:apps",
    "affected:libs": "nx affected:libs",
    "affected:build": "nx affected:build",
    "affected:e2e": "nx affected:e2e",
    "affected:test": "nx affected:test",
    "affected:lint": "nx affected:lint",
    "affected:dep-graph": "nx affected:dep-graph",
    "affected": "nx affected",
    "format": "nx format:write",
    "format:write": "nx format:write",
    "format:check": "nx format:check",
    "update": "nx migrate latest",
    "workspace-schematic": "nx workspace-schematic",
    "dep-graph": "nx dep-graph",
    "help": "nx help",
    "postinstall": "ngcc --properties es2015 browser module main --first-only --create-ivy-entry-points",
    "clean": "npx rimraf -- hooks node_modules package-lock.json && npm i",
    "start.web.myweb": "nx serve web-myweb",
    "start.web.myweb.dev": "nx serve web-myweb --configuration=dev",
    "start.web.myweb.qa": "nx serve web-myweb --configuration=qa",
    "start.nativescript.mobile.ios": "nx run nativescript-mobile:ios",
    "start.nativescript.mobile.ios.dev": "nx run nativescript-mobile:ios --args='--configuration=dev'",
    "start.nativescript.mobile.ios.qa": "nx run nativescript-mobile:ios --args='--configuration=qa'",
    "start.nativescript.mobile.android": "cd apps/nativescript-mobile && ns debug android --no-hmr --emulator",
    "clean.nativescript.mobile": "cd apps/nativescript-mobile && ns clean && npm i && npx rimraf -- package-lock.json",
    "both": "npm-run-all -p start.web.myweb  start.nativescript.mobile.ios",
    "both.dev": "npm-run-all -p start.web.myweb.dev start.nativescript.mobile.ios.dev",
    "both.qa": "npm-run-all -p start.web.myweb.qa start.nativescript.mobile.ios.qa"
  },
  "private": true,
